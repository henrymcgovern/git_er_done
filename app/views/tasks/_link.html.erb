<p>link table below</p>

<table>
	<th>Description</th>
	<th>Created at</th>
	<% @tasks.each do |t| %>
		<tr>
			<td><%= t.description%></td>
			<td><%= t.created_at%></td>
			<td><%= link_to "edit", edit_task_path(t) %></td>
			<td><%= link_to "delete", task_path(t), confirm: "Sure?", method: :delete %></td>
		</tr>
	<% end %>
</table>
